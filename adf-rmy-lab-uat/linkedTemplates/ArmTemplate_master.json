{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-rmy-lab-uat"
		},
		"akv_rmy_lab_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://akv-rmy-lab-sec-uat.vault.azure.net/"
		},
		"ls_sql_lab_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "akv-rmy-lab-sqldb-conn"
		},
		"ir-rmy-lab_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/5982051f-c43a-4684-8691-4fc3a69eb7a7/resourcegroups/rg-rmy-lab-dev/providers/Microsoft.DataFactory/factories/adf-rmy-lab-dev/integrationruntimes/ir-rmy-lab"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-rmy-lab-uat_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"akv_rmy_lab_properties_typeProperties_baseUrl": {
						"value": "[parameters('akv_rmy_lab_properties_typeProperties_baseUrl')]"
					},
					"ir-rmy-lab_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('ir-rmy-lab_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"ls_sql_lab_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_sql_lab_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}