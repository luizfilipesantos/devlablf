				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "sp_executesql",
					"storedProcedureParameters": {
						"statement": {
							"value": "insert into apps.app_001_metas\n(\n    [cd_loja],\n    [cd_competencia],\n    [dt_competencia],\n    [vl_meta],\n    [dt_ini],\n    [dt_fim],\n    [d01],\n    [d02],\n    [d03],\n    [d04],\n    [d05],\n    [d06],\n    [d07],\n    [d08],\n    [d09],\n    [d10]         \n)\nselect \n    stg.loja,\n    stg.competencia,\n    cast(stg.competencia + '01' as date) dt_competencia,\n    cast(replace(stg.meta,',','.') as numeric) meta,\n    convert(datetime, stg.data_ini, 103) data_ini,\n    convert(datetime, stg.data_fim, 103) data_fim,\n    cast(replace(stg.d01,',','.') as numeric) d01,\n    cast(replace(stg.d02,',','.') as numeric) d02,\n    cast(replace(stg.d03,',','.') as numeric) d03,\n    cast(replace(stg.d04,',','.') as numeric) d04,\n    cast(replace(stg.d05,',','.') as numeric) d05,\n    cast(replace(stg.d06,',','.') as numeric) d06,\n    cast(replace(stg.d07,',','.') as numeric) d07,\n    cast(replace(stg.d08,',','.') as numeric) d08,\n    cast(replace(stg.d09,',','.') as numeric) d09,\n    cast(replace(stg.d10,',','.') as numeric) d10\nfrom \n    dbo.stg_metas_csv stg \n    left join apps.app_001_metas tgt \n    on stg.loja = tgt.cd_loja and stg.competencia = tgt.cd_competencia \nwhere \n    tgt.cd_competencia is null "
						}
					}
				},
